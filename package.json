{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "engines": {
    "node": "8.12.0"
  },
  "description": "A platform for users to ask questions and give answers to asked questions.",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "start:dev": "node_modules/.bin/nodemon --exec babel-node app.js --open --hot",
    "start-react": "webpack ",
    "build": "babel-node app.js --out-file dist/app.js",
    "lint": "eslint *.js --fix",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "pretest": "export NODE_ENV=test || SET \"NODE_ENV=test\" && npm run db:setup",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --cache mocha server/**/*.test.js --exit",
    "react:test": "jest view/src/__tests__/**/*.test.js --coverage",
    "test:watch": "npm run react:test --watch",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "db:migrate": "babel-node server/models/databaseModels/model.js",
    "db:seed": "babel-node server/models/databaseModels/seed.db.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PriscillaSam/Stackoverflow-Lite.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PriscillaSam/Stackoverflow-Lite/issues"
  },
  "homepage": "https://github.com/PriscillaSam/Stackoverflow-Lite#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^3.0.0",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validation": "^1.0.2",
    "file-loader": "^2.0.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.23.0",
    "nodemon": "^1.18.7",
    "parse-database-url": "^0.3.0",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-saga": "^0.16.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.21.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-merge": "^4.1.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "request": "^2.88.0",
    "sinon": "^6.1.5",
    "sinon-express-mock": "^2.0.5",
    "react-testing-library": "^5.4.2",
    "redux-mock-store": "^1.5.3",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/view/__mocks__/styleMock.js"
    }
  }
}
